<header>
	<h1>
		<%= link_to '#CAHack' %>
	</h1>
	<div class='menu'>
		<%= link_to 'Hack-a-thons', root_url %> |
		<%= link_to 'Tweets', tweets_url %> |
		<% if @current_user %>
			<%= link_to 'Profile', user_url(@current_user.id) %> |
			<%= link_to 'Members', users_url %> |
			<%= link_to 'Sign Out', session_url %>
		<% else %>
			<%= link_to 'Sign In', new_session_url, class: 'button' %>
		<% end %>
	</div>
</header>